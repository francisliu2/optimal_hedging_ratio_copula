@Misc{Albanese2005,
  author       = {Claudio Albanese and Oliver Chen and Antonio Dallessandro},
  title        = {Dynamic credit correlation modeling},
  howpublished = {Working paper, Imperial College},
  month        = {December},
  year         = {2005},
  abstract     = {In a series of earlier papers, we discussed single name calibration
	for a class of credit barrier models. This article continues along
	that line to include correlation modeling and applications to credit
	baskets and CDOs. We build high dimensional lattice models correlated
	by means of conditional independence, whereby the driving factor
	has an economic cycle interpretation. Since the model is rating-based,
	management constraints for cash-flow CDOs can possibly be taken into
	account. Recovery rates are stochastic and the explicit time dependence
	of model parameters is fairly minimal. Our solution method is based
	on spectral methods which are very precise especially regarding the
	calculation of hedge ratios, while numerically noisy Monte Carlo
	simulations are entirely avoided. To illustrate the model in two
	different correlation regimes, we show calibration examples to the
	January and May 2005 data for CDX tranches, with and without management
	constraints. In both situations, we find that the market price of
	correlation risk can be estimated in such a way to achieve a perfect
	match to CDO tranche spreads while staying consistent with all single
	name CDS curves.},
  file         = {AlbaneseChenDalessandro_DynamicCreditCorrelationModeling.pdf:AlbaneseChenDalessandro_DynamicCreditCorrelationModeling.pdf:PDF},
  groups       = {Credit spread dynamics},
  url          = {http://www3.imperial.ac.uk/pls/portallive/docs/1/1797904.PDF},
}

