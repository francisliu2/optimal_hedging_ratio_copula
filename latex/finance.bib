  title = {Weak convergence of empirical copula processes},
  journal = {Bernoulli},
  year = {2004},
  volume = {10},
  pages = {847--860},
  number = {5},
  month = {October},
  abstract = {Weak convergence of the empirical copula process has been established
	in the case of
	
	independent marginal distributions (Deheuvels, 1979, 1981). Van der
	Vaart and Wellner
	
	(1996) utilize the functional delta-method to show convergence in
	`1([a; b]2) for some
	
	0 < a < b < 1, under restrictions on the distribution functions. We
	extend their results
	
	by proving the weak convergence of this process in `1([0; 1]2) under
	minimal conditions
	
	on the copula function, which coincides with the result earlier obtained
	by Gaenssler
	
	and Stute (1987). It is argued that the condition on the copula function
	is necessary.
	
	The proof uses the functional delta-method and as a consequence, the
	convergence of the
	
	bootstrap counterpart of the empirical copula process follows immediately.
	In addition,
	
	weak convergence of the smoothed empirical copula process is established,
	which has not
	
	been studied so far.},
  file = {FermanianEtAl_WeakConvergenceOfEmpiricalCopulaProcesses.pdf:FermanianEtAl_WeakConvergenceOfEmpiricalCopulaProcesses.pdf:PDF}
}

